# ZSH
export ZSH=$HOME/.oh-my-zsh
ZSH_THEME="robbyrussell"

source $ZSH/oh-my-zsh.sh

# Brew
export PATH="/usr/local/sbin:$PATH"

# .zprofile
source "$HOME/.zprofile"

# Colors
autoload -U colors
colors

# npm
npm set progress=false
